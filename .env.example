# Example environment configuration for Boys State App backend
# Copy this file to .env and update values for your environment

# PostgreSQL connection string used by Prisma
DATABASE_URL="postgresql://USER:PASSWORD@localhost:5432/your_db?schema=public"

# Secret used to sign JWT tokens (minimum 32 characters required)
JWT_SECRET="change-me-to-a-secure-random-string-at-least-32-chars"

# Optional server settings
PORT=3000
NODE_ENV=development
